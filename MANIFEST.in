include LICENSE.txt
exclude .gitignore
exclude .coverage
exclude .travis.yml
include *.rst
include setup.cfg
prune .cache
prune .git
prune build
prune checkpoints
prune dist
prune tests/__pycache__

recursive-exclude *.egg-info *
